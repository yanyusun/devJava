<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="operObj">

    <insert id="add" parameterType="OperObjRe">
        insert into oper_obj_realtion (obj_id, oper_id
        )
        values ( #{objId,jdbcType=INTEGER}, #{operId,jdbcType=INTEGER}
        )
    </insert>

</mapper>